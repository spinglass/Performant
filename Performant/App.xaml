<Application x:Class="Performant.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Color x:Key="ColorConnected">#FF38C921</Color>
        <Color x:Key="ColorIdle">#FF9EA896</Color>
        <Color x:Key="ColorSendError">#FFFFD516</Color>
        <Color x:Key="ColorLost">#FFF11B11</Color>
    </Application.Resources>
</Application>
